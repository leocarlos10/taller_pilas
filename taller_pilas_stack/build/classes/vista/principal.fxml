         <image>
            <Image url="@../imagen/logo.jpg" />
         </image>
      </ImageView>
      <TextField fx:id="text_nombre" layoutX="229.0" layoutY="240.0" promptText="ingresa un nombre" />
      <Button fx:id="btn_ingresar" layoutX="272.0" layoutY="286.0" mnemonicParsing="false" onAction="#event_mostrar_nombre" text="ingresar" />
      <Label fx:id="label_nombre" layoutX="230.0" layoutY="343.0" prefHeight="17.0" prefWidth="149.0" />
